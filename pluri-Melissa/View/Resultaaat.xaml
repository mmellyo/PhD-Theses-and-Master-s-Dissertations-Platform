<Window x:Class="Project.View.Resultaaat"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:uc="clr-namespace:Project.View.userControls"
        xmlns:local="clr-namespace:Project.View"
        mc:Ignorable="d"
        Title="resultaaat" Height="700" Width="1238">

    <Grid>
        <StackPanel Background="#0093ED" Height="75" VerticalAlignment="Top">
        </StackPanel>

        <StackPanel Background="White" Width="200" HorizontalAlignment="Left">
        </StackPanel>

        <ScrollViewer VerticalScrollBarVisibility="Visible"  Background="{x:Null}" BorderBrush="{x:Null}" Foreground="{x:Null}" >
            <ItemsControl 
                ItemsSource="{Binding  TheseService.Theses}"
                VerticalAlignment="Stretch"
                HorizontalAlignment="Stretch">

                <ItemsControl.ItemTemplate>
                    <DataTemplate>
                        <uc:ThesesCard  />
                    </DataTemplate>
                </ItemsControl.ItemTemplate>
            </ItemsControl>
        </ScrollViewer>
    </Grid>
</Window>
