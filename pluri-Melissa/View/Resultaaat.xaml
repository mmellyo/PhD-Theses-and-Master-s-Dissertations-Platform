<Window x:Class="Project.View.Resultaaat"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:uc="clr-namespace:Project.View.userControls"
        xmlns:local="clr-namespace:Project.View"
        mc:Ignorable="d"
        Title="resultaaat" Height="700" Width="1238">

    <Grid>
        <StackPanel Background="#0093ED" Height="75" VerticalAlignment="Top">
        </StackPanel>

        <StackPanel Background="White" Width="200" HorizontalAlignment="Left">
        </StackPanel>

        <Grid Margin="200,75,0,0" Background="#f0f0f0">
            <ListBox Name="resultListBox" ItemsSource="{Binding Results}" Background="{x:Null}" BorderBrush="{x:Null}" Foreground="{x:Null}">
                <ListBox.ItemTemplate>
                    <DataTemplate>
                        <uc:ThesesCard />
                    </DataTemplate>
                </ListBox.ItemTemplate>
            </ListBox>

        </Grid>
    </Grid>
</Window>
