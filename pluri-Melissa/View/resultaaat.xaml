<Window x:Class="gestion.Views.resultaaat"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:gestion.Views"
        mc:Ignorable="d"
        Title="resultaaat" Height="1080" Width="1920">
    <Grid>
        <StackPanel Background="#0093ED" Height="115" VerticalAlignment="Top">

        </StackPanel>

        <StackPanel Background="White" Width="300" HorizontalAlignment="Left">

        </StackPanel>
        <Grid Margin="300,115,0,0" Background="#f0f0f0">
            <Grid Background="#f0f0f0">
                <ScrollViewer VerticalScrollBarVisibility="Auto">
                    <WrapPanel x:Name="ResultatsWrapPanel"
                       Orientation="Vertical"
                       Margin="20" />
                </ScrollViewer>
            </Grid>
        </Grid>
    </Grid>
</Window>
